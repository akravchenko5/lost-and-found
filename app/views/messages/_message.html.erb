 <div class="message-wrap <%= message.user == current_user ? 'self-message' : '' %>">
  <% if message.photo.attached? %>
    <%= cl_image_tag message.photo.key, height: 600, crop: :fit, class: 'chat-image' %>
    <% end %>
    <% if message.content === '' %>

    <% else %>
      <div class="message">
        <%= message.content %>
      </div>
    <% end %>
</div>
